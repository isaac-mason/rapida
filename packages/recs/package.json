{
  "name": "@rapidajs/recs",
  "version": "0.0.7",
  "description": "A flexible entity component system for rapida",
  "homepage": "https://gitlab.com/rapidajs/rapida/-/tree/main/packages/recs",
  "bugs": {
    "url": "https://gitlab.com/rapidajs/rapida/issues"
  },
  "main": "lib/index.js",
  "scripts": {
    "prebuild": "rm -rf ./lib",
    "build:types": "tsc",
    "build:js": "babel src --out-dir lib --extensions \".ts\" --source-maps inline",
    "build": "yarn run build:types && yarn run build:js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts",
    "docs": "typedoc --hideBreadcrumbs --out docs src/index.ts && rm docs/README.md && mv docs/modules.md docs/README.md"
  },
  "author": "Isaac Mason",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.16.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.16.0",
    "@babel/plugin-proposal-numeric-separator": "^7.16.0",
    "@babel/plugin-proposal-optional-chaining": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@types/jest": "^27.0.1",
    "@types/pino": "^6.3.11",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "anysort": "^2.0.0",
    "babel-jest": "^27.3.1",
    "babel-loader": "^8.2.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.0",
    "prettier": "^2.3.2",
    "process": "^0.11.10",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "@jest/globals": "^27.2.0",
    "@rapidajs/rapida-common": "0.0.7",
    "jest-mock": "^27.1.1",
    "ts-jest": "^27.0.5",
    "typedoc": "^0.22.10",
    "typedoc-plugin-markdown": "^3.11.7"
  }
}
