{
  "name": "@rapidajs/rapida",
  "version": "0.0.7",
  "description": "A library that helps you create 3D experiences for the web",
  "homepage": "https://gitlab.com/rapidajs/rapida/-/tree/main/packages/rapida",
  "bugs": {
    "url": "https://gitlab.com/rapidajs/rapida/issues"
  },
  "main": "lib/index.js",
  "scripts": {
    "prebuild": "rm -rf ./lib",
    "build:types": "tsc",
    "build:js": "babel src --out-dir lib --extensions \".ts\" --source-maps inline",
    "build": "yarn run build:types && yarn run build:js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts",
    "storybook": "start-storybook -p 6006",
    "storybook:https": "start-storybook -p 6006 --https --ssl-cert ../../dev/ca.crt --ssl-key ../../dev/ca.key",
    "build-storybook": "build-storybook",
    "docs": "typedoc --hideBreadcrumbs --out docs src/index.ts && rm docs/README.md && mv docs/modules.md docs/README.md"
  },
  "author": "Isaac Mason",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.16.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.16.0",
    "@babel/plugin-proposal-numeric-separator": "^7.16.0",
    "@babel/plugin-proposal-optional-chaining": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@storybook/addon-actions": "^6.4.1",
    "@storybook/addon-docs": "^6.4.1",
    "@storybook/addon-essentials": "^6.4.1",
    "@storybook/addon-links": "^6.4.1",
    "@storybook/addon-storysource": "^6.4.1",
    "@storybook/builder-webpack5": "^6.4.1",
    "@storybook/html": "^6.4.1",
    "@storybook/manager-webpack5": "^6.4.1",
    "@types/jest": "^27.0.1",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "anysort": "^2.0.0",
    "babel-jest": "^27.3.1",
    "babel-loader": "^8.2.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-storybook": "^0.5.1",
    "jest": "^27.2.0",
    "prettier": "^2.3.2",
    "process": "^0.11.10",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "@jest/globals": "^27.2.0",
    "@rapidajs/rapida-common": "0.0.7",
    "@rapidajs/rapida-physics": "0.0.7",
    "@rapidajs/recs": "0.0.7",
    "@types/stats.js": "^0.17.0",
    "@types/three": "^0.133.0",
    "jest-mock": "^27.1.1",
    "stats.js": "^0.17.0",
    "three": "^0.133.0",
    "three-stdlib": "^2.5.4",
    "ts-jest": "^27.0.5",
    "typedoc": "^0.22.10",
    "typedoc-plugin-markdown": "^3.11.7"
  },
  "peerDependencies": {
    "three": "^0.133.0"
  }
}
