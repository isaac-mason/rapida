{
  "name": "@rapidajs/postprocessing",
  "version": "0.0.2",
  "description": "simple postprocessing effects for three js - powered by vanruesc/postprocessing",
  "homepage": "https://gitlab.com/rapidajs/rapida/-/tree/main/packages/postprocessing",
  "bugs": {
    "url": "https://gitlab.com/rapidajs/rapida/issues"
